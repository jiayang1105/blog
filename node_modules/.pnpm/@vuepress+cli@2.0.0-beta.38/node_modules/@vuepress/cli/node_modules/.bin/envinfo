#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/envinfo@7.8.1/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules:/Users/jyy/Desktop/docs-next/node_modules:/Users/jyy/Desktop/node_modules:/Users/jyy/node_modules:/Users/node_modules:/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/@vuepress+cli@2.0.0-beta.38/node_modules/@vuepress/cli/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/@vuepress+cli@2.0.0-beta.38/node_modules/@vuepress/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/@vuepress+cli@2.0.0-beta.38/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/envinfo@7.8.1/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules:/Users/jyy/Desktop/docs-next/node_modules:/Users/jyy/Desktop/node_modules:/Users/jyy/node_modules:/Users/node_modules:/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/@vuepress+cli@2.0.0-beta.38/node_modules/@vuepress/cli/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/@vuepress+cli@2.0.0-beta.38/node_modules/@vuepress/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/@vuepress+cli@2.0.0-beta.38/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../envinfo@7.8.1/node_modules/envinfo/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../envinfo@7.8.1/node_modules/envinfo/dist/cli.js" "$@"
fi
