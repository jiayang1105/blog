#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/dist/bin/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/dist/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/rollup@2.70.2/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules:/Users/jyy/Desktop/docs-next/node_modules:/Users/jyy/Desktop/node_modules:/Users/jyy/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/dist/bin/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/dist/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/rollup@2.70.2/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules/.pnpm/node_modules:/Users/jyy/Desktop/docs-next/blog/node_modules:/Users/jyy/Desktop/docs-next/node_modules:/Users/jyy/Desktop/node_modules:/Users/jyy/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
